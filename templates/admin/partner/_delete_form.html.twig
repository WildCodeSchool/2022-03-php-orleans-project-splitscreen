<form method="post" action="{{ path('partners_delete', {'id': partner.id}) }}"  onsubmit="return confirm('Êtes-vous sûr de bien vouloir supprimer ce partenaire ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ partner.id) }}">
    <button class="btn btn-danger m-2 d-flex">Supprimer<i class="bi bi-trash3-fill ms-2"></i></button>
</form>
